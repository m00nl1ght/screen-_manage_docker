# Временная зона
WORKSPACE_TIMEZONE=Europe/Moscow

# XDEBUG
DOCKER_PHP_ENABLE_XDEBUG='on'

# Настройки Nginx
# Порт, который следует использовать
# для соединения с локального компьютера
NGINX_PORT_SERVER=80
NGINX_PORT_CLIENT=80

# Настройки PHP
# Внешний порт, доступен с локального компьютера
# Контейнеры для backend
PHP_BACKEND_SERVER_PORT=9003
PHP_BACKEND_CLIENT_PORT=9004

# Настройки MySQL 8.x
# Порт, который следует использовать
# для соединения с локального компьютера
MYSQL_SERVER_PORT=4308
MYSQL_CLIENT_PORT=4309

# Настройки общие для MySQL 8.x
MYSQL_ROOT_PASSWORD=secret
MYSQL_DATABASE=test

# Настройки Frontend
# Порт, который следует использовать
# для соединения с локального компьютера
FRONTEND_PORT=8091